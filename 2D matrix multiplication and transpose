a= [[1,2,3],[4,5,6],[7 ,8,9]]
b= [[2,2,2,2],[1,1,1,1],[3,3,3,3]]
for i in range(len(a):
   for j in range(len(b[0])):
      for k in range(len(b)):
           result[i][j] += a[i][k] * b[k][j]
for r in result:
   print(r)
for row in r:
    print(row)
rez = [[r[j][i] for j in range(len(m))] for i in range(len(r[0]))]
print("\n")
for row in rez:
    print(row)
